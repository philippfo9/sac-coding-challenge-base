  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.dev next",
    "dev:mainnet": "env-cmd -f .env.dev-mainnet next",
    "start": "next start",
    "cli:build": "prisma generate",
    "cli:render": "ts-node --project cli/tsconfig.json cli/dipCli.ts",
    "build-check": "prisma generate && npx tsc -p ./ --noEmit --incremental false",
    "build:dev": "env-cmd -f .env.dev prisma generate && next build",
    "cli": "env-cmd -f .env.dev ts-node --project cli/tsconfig.json cli/dipCli.ts",
    "cli:local": "env-cmd -f .env.local ts-node --project cli/tsconfig.json cli/dipCli.ts",
    "cli:test": "env-cmd -f .env.dev ts-node --project cli/tsconfig.json cli/dipCli.ts test",
