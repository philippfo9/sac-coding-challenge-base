    "cli:prod": "env-cmd -f .env.prod ts-node --project cli/tsconfig.json cli/dipCli.ts",
    "db:migrate": "env-cmd -f .env.dev npx prisma migrate dev --name",
    "prim:gen": "prisma generate",
    "db:init:dev": "npm run db:migrate-deploy:dev && prisma generate",
    "db:seed": "env-cmd -f .env.local ts-node --project cli/tsconfig.json cli/dipCli.ts testSeed",
    "db:rename-migrate": "env-cmd -f .env.dev npx prisma migrate dev --name rename-migration --create-only",
    "db:migrate-deploy": "npx prisma migrate deploy",
    "db:migrate-deploy:dev": "env-cmd -f .env.dev npx prisma migrate deploy",
    "db:migrate-deploy:prod": "env-cmd -f .env.prod npx prisma migrate deploy",
    "db:reset:dev": "env-cmd -f .env.dev npx prisma migrate reset --force",
    "db:reset:complete": "rm -rf prisma/migrations/* && npm run db:reset:dev && npm run db:migrate init && npm run db:seed",
    "postinstall": "patch-package",
    "prepare": "cd ../ && husky install ./husky",
    "vercel-build": "prisma generate && yarn db:migrate-deploy && next build",
    "nextInfo": "next info"
  },
